# HealthStack Frontend Environment Variables
# Copy this file to .env.local and fill in your actual values

# Backend API Configuration
VITE_API_URL=https://hs-backend-w6my.onrender.com/
VITE_NODE_ENV=production

# Upload Service URL
VITE_UPLOAD_URL=https://backend.healthstack.africa/upload

# Frontend App URL (for links in emails, etc.)
VITE_APP_URL=https://app.healthstack.africa
VITE_CITIZEN_APP_URL=https://citizen-healthstack.netlify.app

# Google Maps API Key (Get from: https://console.cloud.google.com/)
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Paystack API Keys (Get from: https://dashboard.paystack.com/)
VITE_PAYSTACK_PUBLIC_KEY=your_paystack_public_key_here

# External API URLs
VITE_PURELIFE_API_URL=https://python-api.purelifehealth.io
VITE_SNOMED_API_URL=https://browser.ihtsdotools.org/snowstorm/snomed-ct

# Optional: Alternative backend URLs for development
# VITE_API_URL=http://localhost:8080
# VITE_UPLOAD_URL=http://localhost:8080/upload
